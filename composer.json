        "symfony/framework-bundle": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/yaml": "^7.0",
        "doctrine/orm": "^3.0",
        "symfony/property-access": "^7.3"
    },
    "autoload": {
